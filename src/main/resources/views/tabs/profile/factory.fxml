<?import javafx.geometry.Insets?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.VBox?>

<VBox minHeight="-Infinity" minWidth="-Infinity" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <CheckBox fx:id="enableEnhancementCheckBox" mnemonicParsing="false" text="Enable T-Doll enhancement" />
      <CheckBox fx:id="enableDisassemblyCheckBox" mnemonicParsing="false" text="Enable T-Doll disassembly" />
      <CheckBox fx:id="alwaysDisassembleAfterEnhanceCheckBox" mnemonicParsing="false" text="Always disassemble after enhance" />
      <Separator prefWidth="200.0" />
      <CheckBox fx:id="enableEquipDisassemblyCheckBox" mnemonicParsing="false" text="Enable Equipment disassembly" />
      <CheckBox fx:id="enableEquip4StarDisassemblyCheckBox" mnemonicParsing="false" text="Enable 4 Star Equipment disassembly" />
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</VBox>
